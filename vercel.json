{
  "version": 2,
  "builds": [
    {
      "src": "/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|json|png|jpg|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}